programa
{
	inclua biblioteca Util --> u

	inteiro inicial
	inteiro final
	inteiro aleatorio
	
	funcao inicio()
	{
		escreva (" Digite um numero maior que 10 \n")
		leia (inicial)
		escreva (" Digite outro número maior que  ", inicial + "\n")
		leia (final)
		limpa()
		aleatorio = u.sorteia(inicial, final)
		se  (inicial >= 10 e final <=100){
		escreva (" Número gerado foi ", aleatorio )
		}
		senao{
		 	escreva (" Erro digite os números novamente ")
		}
		
		
		
	}
}
