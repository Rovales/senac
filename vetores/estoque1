programa
{
    cadeia nomProduto[5]
    inteiro quantidade[5]
    funcao inicio()
{
    // AQUI PREENCHE OS VETORES  
    para(inteiro i = 0; i < 5; i++) {
    escreva("Digite o nome do produto: ")
    leia(nomProduto[i])
    escreva("Digite a quantidade em estoque: ")
    leia(quantidade[i])
}
// MOSTRAR A QUANTIDADE NO CONSOLE
    escreva("nomProduto\tQtdeEstoque\n")
    para(inteiro i = 0; i < 5; i++) {
    escreva(nomProduto[i] + "\t\t" + quantidade[i] + "\n")
        }
    }
}
